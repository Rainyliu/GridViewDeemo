<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rainy.gridviewdeemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".FloatingActionButtonActivity" />
        <activity android:name=".DrawBitmapActivity" />
        <activity android:name=".BezierActivity" />
        <activity android:name=".SvgActivity" />
        <activity android:name=".AnimatorActivity" />
        <activity android:name=".DecodeImageActivity" />
        <activity android:name=".TwinklingRefreshLayoutActivity" />
        <activity android:name=".ButterKnifeActivity"></activity>
        <activity android:name=".RetrofitDemoActivity">
        </activity>
        <activity android:name=".PhotoViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>